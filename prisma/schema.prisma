generator client {
  provider = "prisma-client-js"
}


datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}
 
model Video {
  id Int @id @default(autoincrement())
  filename String @unique
  title String
}

model User {
  id Int @id @default(autoincrement())
  email String @unique
  password String
  role Role @default(USER)
  emailVerified Boolean @default(false)
  subscribed Boolean @default(false)
}

enum Role {
  USER
  ADMIN
}